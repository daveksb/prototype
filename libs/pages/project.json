{
  "projectType": "library",
  "root": "libs/pages",
  "sourceRoot": "libs/pages/src",
  "prefix": "prototype",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/pages"],
      "options": {
        "jestConfig": "libs/pages/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/pages/src/**/*.ts",
          "libs/pages/src/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
