// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
  //url    = env("DATABASE_URL")
}

model system_flow {
  id          String @id @default(cuid())
  flow_name   String
  flow_order  Int
  page        String
  approval_id Int
}
