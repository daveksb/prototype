// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
  //url    = env("DATABASE_URL")
}

model SystemFlow {
  id          String @id @default(cuid())
  flow_name   String
  flow_order  Int
  page        String
  service_id  String
}

model Service {
  id          String @id @default(cuid())
  details     String
  approval_id String
}

model Approval {
  id          String @id @default(cuid())
  approver    String
}
